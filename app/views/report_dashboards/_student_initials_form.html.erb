<%= simple_form_for :student_initials_selections, url: date_report_path, method: :get do |f| %>

<%= f.input :initials,
  collection: student_initials,
  as: :check_boxes,
  label: false,
  input_html: { class: 'multiselectinitials', multiple: true },
  include_hidden: false %>

  <%= f.input :filtered_check_ids, as: :hidden, input_html: { value: filtered_check_ids }%>
  <%= f.button :submit, value: "Next Filter" %>
<% end %>
